{
  "extends": ["stylelint-config-standard", "stylelint-config-clean-order"],
  "plugins": ["stylelint-no-unsupported-browser-features"],
  "rules": {
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": ["label"]
      }
    ],

    "plugin/no-unsupported-browser-features": [
      true,
      {
        "severity": "warning"
      }
    ]
  }
}
